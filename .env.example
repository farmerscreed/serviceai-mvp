# .env.example
# Copy this to .env.local and fill in your values

#===========================================
# Supabase Configuration
#===========================================
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

#===========================================
# Vapi.ai Configuration
#===========================================
VAPI_API_KEY=
NEXT_PUBLIC_VAPI_BASE_URL=https://api.vapi.ai
VAPI_DEMO_ASSISTANT_ID=
VAPI_DEMO_PHONE_NUMBER_ID=

#===========================================
# Twilio Configuration
#===========================================
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=
NEXT_PUBLIC_TWILIO_WEBHOOK_BASE_URL=

#===========================================
# Application Configuration
#===========================================
NEXT_PUBLIC_APP_NAME=ServiceAI
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

#===========================================
# Multi-Language Configuration
#===========================================
NEXT_PUBLIC_DEFAULT_LANGUAGE=en
NEXT_PUBLIC_SUPPORTED_LANGUAGES=en,es

#===========================================
# Webhook Configuration
#===========================================
NEXT_PUBLIC_WEBHOOK_BASE_URL=http://localhost:3000/api/webhooks
VAPI_WEBHOOK_SECRET=
TWILIO_WEBHOOK_SECRET=

#===========================================
# Feature Flags
#===========================================
NEXT_PUBLIC_ENABLE_MULTI_LANGUAGE=true
NEXT_PUBLIC_ENABLE_SMS=true
NEXT_PUBLIC_ENABLE_VOICE_AI=true

#===========================================
# Development Tools
#===========================================
NEXT_PUBLIC_ENABLE_DEBUG_LOGGING=true
